{
    "name": "fengche/payment",
    "type": "library",
    "description": "支持hyperf 支付宝支付、微信支付、招商一网通支付php SDK。方便快速接入，最完整的开源支付 php sdk",
    "keywords": ["alipay", "weixin", "支付宝支付", "微信支付", "集成支付接口SDK", "招商一网通", "一网通"],
    "homepage": "https://github.com/ZMKing/payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Leo",
            "email": "dayugog@gmail.com",
            "homepage": "https://dayutalk.cn"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-bcmath": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "~7.0",
        "guzzlehttp/uri-template": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "*"
    },
    "autoload": {
        "psr-4": {"Payment\\": "src/"}
    },
    "bin": [
        "bin/CertificateDownloader.php"
    ],
    "scripts": {
        "v3-certificates": "bin/CertificateDownloader.php"
    }
}
